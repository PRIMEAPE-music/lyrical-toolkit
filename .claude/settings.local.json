{
  "permissions": {
    "allow": [
      "Bash(mkdir:*)",
      "Bash(dir)",
      "Bash(ren:*)",
      "Bash(mv:*)",
      "Bash(npm install:*)",
      "Bash(for file in *.js)",
      "Bash(do echo \"Checking $file:\")",
      "Bash(node:*)",
      "Bash(done)",
      "Bash(curl:*)",
      "Bash(npm start)",
      "Bash(npx kill-port:*)",
      "Bash(PORT=3001 npm start)",
      "Bash(git fetch:*)",
      "Bash(git pull:*)",
      "Bash(git add:*)"
    ],
    "defaultMode": "acceptEdits"
  }
}